{
  "name": "knue-board-hub",
  "version": "1.0.0",
  "description": "Korea National University of Education RSS Board Aggregator",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "npx wrangler dev",
    "dev:full": "concurrently \"npm run dev:worker\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "fast-xml-parser": "^5.2.5",
    "lz-string": "^1.5.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.154",
    "@types/node": "^20.10.0",
    "@unocss/postcss": "^66.3.3",
    "@unocss/preset-attributify": "^0.58.0",
    "@unocss/preset-icons": "^0.58.0",
    "@unocss/preset-wind": "^0.58.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^9.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "hono": "^4.8.9",
    "typescript": "~5.3.0",
    "unocss": "^66.3.3",
    "vite": "^7.0.6",
    "vite-plugin-pwa": "^1.0.2",
    "vue-tsc": "^3.0.4"
  },
  "keywords": [
    "knue",
    "rss",
    "vue3",
    "mobile",
    "korea",
    "education",
    "university",
    "bulletin",
    "board"
  ],
  "author": "KNUE Board Hub Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/knue/board-hub.git"
  },
  "homepage": "https://knue.ac.kr/board-hub",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
